package{		import flash.display.MovieClip;	import flash.net.*;	import flash.events.*;			public class Main extends MovieClip{				private var nc:NetConnection;        private var rtmpNow:String;        private var msg:String;		private var loader:URLLoader;		private var Data:XMLList;		private var CurrentPlaying:Array;		private var gender:Boolean;				public function Main(){			 gender = true;			 nc=new NetConnection();             nc.addEventListener (NetStatusEvent.NET_STATUS,checkConnect);             rtmpNow="rtmpe://192.168.1.111/connect";             //rtmpNow="rtmpe:/connect";             //nc.connect (rtmpNow);			 			 //OP = OutputPanel Instance			 //VC = VideoController Instance			 			 //temp loader...try to load file directly trough gateway			loader = new URLLoader();			loader.addEventListener(Event.COMPLETE, onload);			loader.addEventListener(IOErrorEvent.IO_ERROR, error);			loader.load(new URLRequest("CoronaDataSheet.xml"));		}		public function error(event:IOErrorEvent){						trace("Main : "+event.target.message);					}		private function onload(event:Event){						//When data arrives			Data = new XMLList(event.target.data);			//trace(Data);			MovieClip(this).VC.loadBoard(Data.Scoreboard.(@id=="women"));			MovieClip(this).VC.loadList();			MovieClip(this).OP.loadDraft(Data.Draft.(@id=="women"));					}		public function setCurrent(c:Array){						CurrentPlaying = new Array();			CurrentPlaying = c;			//trace(CurrentPlaying.toString());				}		public function getCurrent():Array{			return CurrentPlaying;		}		public function bounceData(d:XMLList,command:String){						//This comunicates Video Controller with Output Panel						if(command=="click"){								OP.confirm(d);							}			else if(command=="roll"){								OP.recive(d);						}			else if(command=="out"){								OP.IP.removePreview();			}					}		private function checkConnect (event:NetStatusEvent):void        {			//Media Server Communication			msg=event.info.code;			trace(msg);		}	}	}