package OutputPanel{		import flash.display.MovieClip;	import flash.text.TextField;	import flash.text.TextFormat;	import flash.text.TextFieldAutoSize;	import flash.filters.DropShadowFilter;		public class GraphicContainer extends MovieClip{				private var Id:Array;		private var riders:Array;		private var On:Boolean;						public function GraphicContainer (){			On=false;				}		public function loadGraphics(g:Array, i:Array){			On = true;			Id = new Array();			riders = new Array();			riders = g;			Id = i;									}			}	}